{
  "name": "jasmine-snapshot",
  "version": "1.8.1",
  "description": "A snapshotting library similar to what jest offers but for Jasmine 2 instead. Written in Typescript",
  "main": "lib/jasmine-snapshot.min.js",
  "typings": [
    "index.d.ts"
  ],
  "scripts": {
    "build": "webpack --env build -p",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest",
    "example:test": "karma start"
  },
  "dependencies": {
    "@types/vkbeautify": "^0.99.1",
    "@types/node": "^7.0.18",
    "difflib": "^0.2.4",
    "vkbeautify": "^0.99.2",
    "x2js": "^3.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "jasmine-core": "^2.99.0",
    "jest": "^22.4.3",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-typescript": "^3.0.1",
    "ts-jest": "^22.4.5",
    "ts-loader": "^2.0.3",
    "tslint": "*",
    "typescript": "*",
    "webpack": "^2.3.3",
    "yargs": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/horvay/jasmine-snapshot"
  },
  "keywords": [
    "snapshot",
    "jest",
    "jasmine",
    "karma",
    "browser",
    "test",
    "expected"
  ],
  "author": "Horvay, Greg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/horvay/jasmine-snapshot"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/__tests__/**/*.spec.ts"
    ],
    "collectCoverageFrom": [
      "src/index.ts"
    ]
  }
}
